package com.example.demo.model;

import java.math.BigDecimal;

import lombok.Data;

public class Book {
private Integer id;
private String name;
private Integer quantity;
private BigDecimal price;

public Book(Integer id,String name,Integer quantity,BigDecimal price) {
	super();
	this.id = id;
	this.name = name;
	this.quantity = quantity;
	this.price = price;
}

}
